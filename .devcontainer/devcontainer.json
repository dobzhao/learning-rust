{
	"name": "Codespaces Rust Starter",
	"customizations": {
		"vscode": {
			"extensions": [
				"bungcip.better-toml",
				"rust-lang.rust-analyzer",
				"serayuzgur.crates",
				"eamodio.gitlens",
				"vadimcn.vscode-lldb"
			],
			"settings": {
				"editor.formatOnSave": true,
				"terminal.integrated.defaultProfile.linux": "zsh",
				"files.exclude": {
					"**/LICENSE": true
				}
			}
		}
	},
	"dockerFile": "Dockerfile"
}